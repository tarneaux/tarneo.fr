@charset "UTF-8";html{--bg-color:#ffffff;--fg-color:#000000;--link-color:#000000;--emphasis-color:#000000;--nav-bg-color:#ffffff;--nav-shadow:0 0 1em rgba(0, 0, 0, 0.25);--nav-sub-bg-color:#eee;--selection-background-color:#000000;--selection-foreground-color:#ffffff}@media(prefers-color-scheme:dark){html{--bg-color:#1e1e1e;--fg-color:#b8bcb9;--link-color:#fb0067;--emphasis-color:#c37033;--nav-bg-color:#3a3c43;--nav-shadow:none;--nav-sub-bg-color:#292c31;--selection-background-color:#292c31;--selection-foreground-color:#1e1e1e}}body{background-color:var(--bg-color);color:var(--fg-color);font-family:monospace}h1,h2,h3,h4,h5,h6{margin:0;font-weight:700;color:var(--emphasis-color)}article>h1{font-size:1.5em}article>h2{font-size:1.25em}article>h3{font-size:1.125em}article>h4{font-size:1.0625em}article>h5{font-size:1.03125em}article>h6{font-size:1em}b,strong{color:var(--emphasis-color)}a{color:var(--link-color)}img{border-radius:5px}ul>li::marker{color:var(--emphasis-color);content:"- "}sup{line-height:0}::selection{background-color:var(--selection-background-color);color:var(--selection-foreground-color)}div.footnote-definition>:first-child::after{content:" ───────────";overflow:hidden;color:var(--emphasis-color)}div.footnote-definition>sup{margin-right:.5em}div.footnote-definition>*{margin:.5em 0}div.footnote-definition>sup.footnote-definition-label{color:var(--emphasis-color);font-weight:400;font-size:1em;vertical-align:baseline}h1.section-title,h1.page-title{display:flex;align-items:end;justify-content:space-between}h1.section-title>img,h1.page-title>img{height:2em;margin:0;padding:0}.blink{animation:blink 1s steps(1)infinite;color:var(--emphasis-color)}footer{opacity:.5}body{margin:0;margin-top:var(--nav-height);padding:4em 0;min-height:calc(100vh - 8em - var(--nav-height));display:flex;flex-direction:column;justify-content:center;align-items:center}#parent{display:flex;justify-content:center;align-items:center;flex-direction:column;flex-grow:1}#child{max-width:100%;width:Min(65ch,95vw)}#child img{max-width:100%}#navbar{position:fixed;top:0;left:0;right:0;height:var(--nav-height);background-color:var(--nav-bg-color);box-shadow:var(--nav-shadow);display:flex;justify-content:center;align-items:center}#navbar a{display:inline-block;padding:0 .5em;font-weight:700}#navbar .sub{font-weight:400}#navbar #sub-box{padding:.2em;border-radius:.2em;background-color:var(--nav-sub-bg-color);margin-left:.5em;margin-right:.5em}html{--nav-height:2em}